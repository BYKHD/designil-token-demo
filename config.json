{
    "source": ["transtokens.json"],
    "platforms": {
      "scss": {
        "transformGroup": "scss",
        "buildPath": "build/scss/",
        "files": [{
          "destination": "_variables.scss",
          "format": "scss/variables"
        }]
      },
      "less":{
        "transformGroup":"less",
        "buildPath": "build/less/",
        "files": [{
          "destination": "customcolor.less",
          "format": "less/variables"
        }]
      },
      "js": {
        "transformGroup": "js",
        "buildPath": "build/js/",
        "files": [{
          "destination": "variables.js",
          "format": "javascript/es6"
        }]
      }
    }
}
  